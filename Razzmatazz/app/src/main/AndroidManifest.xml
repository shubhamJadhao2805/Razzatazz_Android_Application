<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="devlight.razzmatzz.sample">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="devlight.razzmatzz.sample.dayThree"></activity>
        <activity android:name="devlight.razzmatzz.sample.dayTwoActivity" />
        <activity android:name="devlight.razzmatzz.sample.DayoneActivity" />
        <activity android:name="devlight.razzmatzz.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="devlight.razzmatzz.sample.HorizontalNtbActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity android:name="devlight.razzmatzz.sample.TopHorizontalNtbActivity" />
        <activity android:name="devlight.razzmatzz.sample.HorizontalCoordinatorNtbActivity" />
        <activity android:name="devlight.razzmatzz.sample.VerticalNtbActivity" />
        <activity android:name="devlight.razzmatzz.sample.SamplesNtbActivity" />
    </application>

</manifest>